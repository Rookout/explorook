{
  "name": "explorook",
  "version": "1.0.3",
  "description": "Rookout's site addon to support local files and folders",
  "main": "dist/index.js",
  "scripts": {
    "publish": "build.sh && build --win -p always",
    "build": "tsc",
    "build-linux": "build --linux APPIMAGE",
    "build-windows": "build --win",
    "dist": "build",
    "mac-publish": "export ELECTRON_BUILDER_ALLOW_UNRESOLVED_DEPENDENCIES=true && build --mac --win --linux -p always",
    "watch": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "yarn run build && cross-enZv development=1 electron ./dist/index.js",
    "debug": "yarn run build && cross-env development=1 electron --inspect-brk ./dist/index.js"
  },
  "build": {
    "productName": "Explorook",
    "extends": null,
    "appId": "com.rookout.explorook",
    "publish": [
      {
        "provider": "github",
        "owner": "rookout",
        "repo": "explorook"
      },
      {
        "provider": "generic",
        "url": "https://get.rookout.com/${name}/${os}/${arch}/"
      }
    ],
    "directories": {
      "output": "installers"
    },
    "files": [
      "dist",
      "assets",
      "graphql",
      "index-worker.html",
      "assets/icons/**/*"
    ],
    "appImage": {
      "systemIntegration": "doNotAsk"
    },
    "linux": {
      "target": "appImage",
      "category": "Utility"
    },
    "dmg": {
      "background": "assets/dmg-background.png"
    },
    "mac": {
      "icon": "assets/icons/mac/icons.icns"
    },
    "win": {
      "publisherName": [
        "Rookout LTD"
      ],
      "target": "NSIS",
      "sign": "./sign_windows.js",
      "icon": "assets/icons/logo@512x512.png"
    }
  },
  "publish-gcs": {
    "bucket": "get.rookout.com",
    "path": "/${name}/${os}/${arch}/${filename}",
    "serviceAccount": "/Users/distiller/project/gcloud_service_account.json",
    "public": true,
    "resumable": false
  },
  "repository": "https://github.com/rookout/explorook",
  "keywords": [
    "Rookout",
    "Explorer",
    "ExploRook",
    "Simple-Https"
  ],
  "author": {
    "email": "support@rookout.com",
    "name": "Rookout"
  },
  "license": "MIT",
  "devDependencies": {
    "@bugsnag/core": "^5.2.0",
    "@material-ui/core": "^1.2.1",
    "@material-ui/icons": "^1.1.0",
    "@playlyfe/gql": "^2.6.0",
    "@types/analytics-node": "^3.1.1",
    "@types/auto-launch": "^5.0.0",
    "@types/aws-lambda": "^8.10.17",
    "@types/body-parser": "^1.17.0",
    "@types/electron-store": "^1.3.0",
    "@types/lodash": "^4.14.109",
    "@types/ws": "^5.1.2",
    "cross-env": "^5.2.0",
    "electron": "^2.0.8",
    "electron-builder": "^20.19.2",
    "electron-builder-lib": "^20.15.3",
    "electron-publisher-gcs": "^0.1.5",
    "graphql-middleware": "^2.0.2",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "@bugsnag/js": "^5.2.0",
    "analytics-node": "^3.4.0-beta.1",
    "args-parser": "^1.1.0",
    "auto-launch": "^5.0.5",
    "body-parser": "^1.19.0",
    "chrome-opn": "^1.0.1",
    "cors": "^2.8.5",
    "electron-log": "^2.2.15",
    "electron-store": "^2.0.0",
    "electron-updater": "2.21.10",
    "graphql-yoga": "^1.16.9",
    "isomorphic-git": "^0.37.3",
    "lodash": "^4.17.10",
    "node-rsa": "^1.0.5",
    "parse-repo": "^1.0.4",
    "slash": "^2.0.0",
    "uuid": "^3.2.1",
    "walk": "^2.3.13"
  }
}
