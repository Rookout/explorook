{
  "name": "explorook",
  "version": "1.7.19",
  "description": "Rookout's site addon to support local files and folders",
  "main": "dist/index.js",
  "scripts": {
    "postinstall": "patch-package",
    "build": "webpack --config webpack.main.config.js && webpack --config webpack.index-worker.config.js",
    "postbuild": "copyfiles index-worker.html ./dist/",
    "package-linux": "electron-builder --linux APPIMAGE",
    "package-windows": "electron-builder --win",
    "build-headless": "tsc",
    "dist": "electron-builder",
    "mac-publish": "export ELECTRON_BUILDER_ALLOW_UNRESOLVED_DEPENDENCIES=true && electron-builder --mac --win --linux -p always",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "cross-env development=1 yarn run build && cross-env development=1 electron ./dist/index.js",
    "debug": "cross-env development=1 $NODE_DEBUG_OPTION yarn run build && cross-env development=1 electron --inspect-brk ./dist/index.js"
  },
  "build": {
    "productName": "Rookout Desktop App",
    "extends": null,
    "appId": "com.rookout.explorook",
    "afterSign": "./afterSignHook.js",
    "publish": [
      {
        "provider": "github",
        "owner": "rookout",
        "repo": "explorook"
      }
    ],
    "directories": {
      "output": "installers"
    },
    "files": [
      "dist",
      "assets",
      "graphql",
      "index-worker.html",
      "assets/icons/**/*",
      "!**/node_modules/**/*"
    ],
    "linux": {
      "target": "appImage",
      "category": "Utility"
    },
    "dmg": {
      "background": "assets/dmg-background.png"
    },
    "mac": {
      "icon": "assets/icons/mac/icons.icns"
    },
    "win": {
      "publisherName": [
        "Rookout LTD"
      ],
      "target": "NSIS",
      "sign": "./sign_windows.js",
      "icon": "assets/icons/logo@512x512.png"
    }
  },
  "repository": "https://github.com/rookout/explorook",
  "keywords": [
    "Rookout",
    "Explorer",
    "ExploRook",
    "Simple-Https"
  ],
  "author": {
    "email": "support@rookout.com",
    "name": "Rookout"
  },
  "license": "MIT",
  "devDependencies": {
    "@bugsnag/core": "6.5.0",
    "@material-ui/core": "^1.2.1",
    "@material-ui/icons": "^1.1.0",
    "@playlyfe/gql": "^2.6.0",
    "@types/analytics-node": "^3.1.1",
    "@types/auto-launch": "^5.0.0",
    "@types/aws-lambda": "^8.10.17",
    "@types/body-parser": "^1.17.0",
    "@types/lodash": "^4.14.109",
    "@types/ws": "^5.1.2",
    "aws-lambda": "^1.0.5",
    "copyfiles": "^2.3.0",
    "cross-env": "^5.2.0",
    "electron": "^8.0.1",
    "electron-builder": "^22.3.2",
    "electron-builder-lib": "^20.15.3",
    "electron-notarize": "^0.2.1",
    "graphql-middleware": "^3.0.2",
    "ts-loader": "^6.0.4",
    "tslint": "^5.10.0",
    "typescript": "^3.5.3",
    "webpack": "4.44.2",
    "webpack-bugsnag-plugins": "^1.4.3",
    "webpack-cli": "^3.3.6",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@bugsnag/js": "6.5.2",
    "@codingame/monaco-jsonrpc": "^0.3.0",
    "@types/valid-url": "^1.0.2",
    "analytics-node": "^3.4.0-beta.1",
    "apollo-server-core": "^2.19.1",
    "apollo-server-express": "^2.19.1",
    "args-parser": "^1.1.0",
    "auto-launch": "^5.0.5",
    "body-parser": "^1.19.0",
    "chrome-opn": "^1.0.1",
    "cors": "^2.8.5",
    "electron-log": "^2.2.15",
    "electron-store": "^5.1.1",
    "electron-updater": "^4.2.2",
    "express": "^4.17.1",
    "folder-delete": "^1.0.4",
    "git-url-parse": "^11.1.2",
    "graphql": "^15.4.0",
    "is-git-url": "^1.0.0",
    "isomorphic-git": "^1.4.0",
    "lodash": "^4.17.20",
    "log4js": "^6.3.0",
    "node-rsa": "^1.0.5",
    "p4api": "^3.3.1",
    "parse-repo": "^1.0.4",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0",
    "slash": "^2.0.0",
    "url-assembler": "^2.1.1",
    "uuid": "^3.2.1",
    "valid-url": "^1.0.9",
    "walk": "^2.3.13"
  },
  "resolutions": {
    "@playlyfe/gql/lodash": "^4.17.15",
    "graphql-tools": "4.0.7"
  }
}
